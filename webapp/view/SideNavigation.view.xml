<mvc:View controllerName="sap.ui.agi.timeRecording.controller.SideNavigation"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:tnt="sap.tnt" height="100%" class="side-navigation-demo">
    <FlexBox alignItems="Start" justifyContent="End" height="100%">
        <ScrollContainer vertical="true" horizontal="false" height="100%">
            <tnt:SideNavigation id="sideNavigation" selectedKey="">
                <tnt:NavigationList items="{user>/eligibleTags}">
                    <tnt:NavigationListItem text="{user>name}"/>
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </ScrollContainer>
    </FlexBox>
</mvc:View>